	"tokenColors": [
		{
			"scope": [
				"entity.name",
				"entity.name.class",
				"entity.name.function",
				"entity.name.section",
				"entity.other.inherited-class",
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#6E47BE"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#D53C4C"
			}
		},
		{
			"scope": [
				"variable",
				"variable.language",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#25292E"
			}
		},
		{
			"scope": [
				"constant",
				"constant.numeric",
				"constant.language",
				"constant.character",
				"constant.character.escape",
				"constant.other"
			],
			"settings": {
				"foreground": "#0E5FC1"
			}
		},
		{
			"scope": [
				"storage",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#D53C4C"
			}
		},
		{
			"scope": "support",
			"settings": {
				"foreground": "#D53C4C"
			}
		},
		{
			"scope": [
				"keyword",
				"keyword.control"
			],
			"settings": {
				"foreground": "#D53C4C"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#25292E"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#063160"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#6A737C"
			}
		},
		{
			"scope": [
				"invalid",
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#D53C4C"
			}
		},
		{
			"scope": [
				"entity.name.function",
				"support.function",
				"support.constant.handlebars"
			],
			"settings": {
				"foreground": "#6E47BE"
			}
		}
	]
}